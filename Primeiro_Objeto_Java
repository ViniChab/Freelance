package aulaobjeto;
public class AulaObjeto 
{
    public static void main(String[] args)
    {
        Caneta c1 = new Caneta();
        c1.cor = "vermelha";
        c1.carga = 100;
        c1.ponta = 0.5f;
        c1.tampada = false;
        c1.modelo = "AK-47";
        c1.status();
        c1.tampar();
        c1.status();
        c1.rabiscar();
        c1.destampar();
        c1.rabiscar();
        c1.status();
    }
}
